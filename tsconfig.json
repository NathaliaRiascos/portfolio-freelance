{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@/components": ["src/components"],
      "@/sections": ["src/components/sections"],
      "@/layouts": ["src/layouts"],
      "@/styles": ["src/styles"],
      "@/icons": ["src/icons"],
      "@/assets": ["src/assets"]
    }
  }
}
